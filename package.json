{
  "name": "logic",
  "version": "1.0.0",
  "description": "A series of studies in logic",
  "scripts": {
    "test": "mocha -w tests/logic tests/drt",
    "logic": "pegjs src/logic/logic.pegjs",
    "drt": "nearleyc src/drt/english.ne -o src/drt/english.js",
    "docs": "nearley-railroad  src/drt/english.ne > tests/drt/grammar.html",
    "build": "browserify -p tinyify --s module src/drt/browser.js -o bin/bundle.js",
    "debug": "browserify --s module src/drt/browser.js -o bin/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelgoto/logic.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/samuelgoto/logic/issues"
  },
  "homepage": "https://github.com/samuelgoto/logic",
  "dependencies": {
    "en-inflectors": "^1.0.12",
    "nearley": "^2.18.0",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "minifyify": "^7.3.5",
    "mocha": "^5.2.0",
    "tinyify": "^3.0.0"
  }
}
